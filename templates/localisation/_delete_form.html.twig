{% extends "back.html.twig" %}
{% block main %}
<form method="post" action="{{ path('app_localisation_delete', {'id': localisation.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ localisation.id) }}">
    <button class="btn">Delete</button>

</form>
{% endblock %}
